<% provide :title, t(".title") %>
<h1><%= t ".list" %></h1>
<div>
  <%= link_to t(".create"), new_admin_subject_path %>
</div>
<table class="table table-striped table-hover">
  <tr>
    <th><%= t ".subject_name" %></th>
    <th><%= t ".description" %></th>
    <th><%= t ".picture" %></th>
    <th><%= t ".time_start" %></th>
    <th><%= t ".time_end" %></th>
    <th><%= t ".status" %></th>
    <th><%= t ".edit" %></th>
    <th><%= t ".delete" %></th>
  </tr>
  <%= render partial: "admin/subjects/subject", collection: @subjects %>
</table>
<%= will_paginate %>
