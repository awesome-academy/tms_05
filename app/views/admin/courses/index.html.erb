<div class="container-fluid">
  <div class="row justify-content-center">
    <div class="col-md-2">
      <%= link_to  t(".creat"), new_admin_course_path, class: "btn btn-block btn-success btn-lg" %>
    </div>
    <div class="col-md-12">
      <h2 class="bg-primary"><%= t(".title") %> </h2>
      <table class="table">
        <tr>
          <th></th>
          <th><%= t(".name") %></th>
          <th><%= t(".time_start") %></th>
          <th><%= t(".time_end") %></th>
          <th><%= t(".created_at") %></th>
          <th><%= t(".status") %></th>
          <th><%= t(".creator") %></th>
        </tr>
        <tr>
          <%= render @courses %>
        </tr>
      </table>
      <%= will_paginate @courses %>
    </div>
  </div>
</div>
